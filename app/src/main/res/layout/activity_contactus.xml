<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#272523"
    tools:context=".Contactus">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="44dp"
        android:gravity="center_horizontal"
        android:text="Contact Us"
        android:textColor="@android:color/background_light"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="374dp"
        android:layout_height="170dp"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="18dp">
        <RelativeLayout
            android:layout_width="374dp"
            android:layout_height="170dp">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="121dp"
                android:layout_height="154dp"
                android:layout_marginEnd="-121dp"
                android:layout_marginRight="-121dp"
                app:srcCompat="@drawable/manav" />

            <TextView
            android:id="@+id/textView12"
            android:layout_width="253dp"
            android:layout_height="60dp"
                android:layout_marginLeft="120dp"
            android:gravity="center"
            android:text="Manav Peshwani"
            android:textColor="@android:color/background_light"
            android:textSize="21dp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="249dp"
                android:layout_height="46dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="70dp"
                android:gravity="center"
                android:paddingLeft="8dp"
                android:text="+91 7798401391"
                android:textColor="#0661D6"
                android:textSize="21sp" />


        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="374dp"
        android:layout_height="170dp"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="18dp">
        <RelativeLayout
            android:layout_width="374dp"
            android:layout_height="170dp">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="121dp"
                android:layout_height="154dp"
                android:layout_marginEnd="-121dp"
                android:layout_marginRight="-121dp"
                app:srcCompat="@drawable/shreya" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="253dp"
                android:layout_height="60dp"
                android:layout_marginLeft="120dp"
                android:gravity="center"
                android:text="Shreya Lanjewar"
                android:textColor="@android:color/background_light"
                android:textSize="21dp" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="249dp"
                android:layout_height="46dp"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="70dp"
                android:gravity="center"
                android:paddingLeft="8dp"
                android:text="+91 8999412539"
                android:textColor="#0661D6"
                android:textSize="21sp" />


        </RelativeLayout>
    </LinearLayout>
</LinearLayout>